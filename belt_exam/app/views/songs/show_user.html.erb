<h3 style="display:inline-block" ><a href="/">Dashboard</a></h3>

<h2><%= @user.first_name%> <%= @user.last_name%>'s Playlist </h2>

<table>
<tr>
    <th>Artist</th>
    <th>Title</th>
    <th>Times Added</th>
</tr>
    <% if @all_songs %>
    <% @all_songs.each do |song|%>
    <tr>
        <td><%= song.artist%></td>
        <td><a href="/songs/<%= song.id %>"><%= song.title %></a></td>
        <td><%= song.likes.where("user_id = ?", @user.id).count %>
    </tr>
    <% end %>
    <%end %>



</table>
